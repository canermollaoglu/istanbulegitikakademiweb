@model NitelikliBilisim.Core.ViewModels.areas.educator_area.payment.MyPaymentsVm
@{
    ViewData["Title"] = "Ödemelerim";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<main>
    <div class="container margin_60_35">

    </div>
    <div class="container margin_60_35">
        <div class="row">
            <div class="col-lg-12">

            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <ul>
                    @foreach (var item in Model.Payments)
                    {
                        <li>
                            <b>@item.GroupName</b>
                            <p>@item.Paid</p>
                        </li>
                    }
                </ul>
            </div>
        </div>
    </div>
</main>
