@{
    ViewData["Title"] = $"Hata";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section styles{
    <style type="text/css">
        body {
            background-image: url(data:image/png;base64,);
        }

        .error-template {
            padding: 40px 15px;
            text-align: center;
        }

        .error-actions {
            margin-top: 15px;
            margin-bottom: 15px;
        }

            .error-actions .btn {
                margin-right: 10px;
            }
    </style>

}
<main>
    <input type="hidden" value="0" id="input-education-total-price" />
    <input type="hidden" value="0" id="promotion-discount-amount" />
    <section id="hero_in" class="cart_section">
        <div class="wrapper">
            <div class="container">
                <div class="bs-wizard clearfix">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="error-template">
                                <h1>
                                    404 !
                                </h1>
                                <div class="error-details">
                                    Ulaşmaya çalıştığınız sayfa değiştirilmiş veya silinmiş olabilir.
                                </div>
                                <hr />
                                <div class="error-actions">
                                    <a href="@Url.Action("Index","Home")" class="btn btn-default">
                                        <span class="glyphicon glyphicon-home"></span>
                                        Anasayfa
                                    </a><a href="mailto:info@niteliklibilisim.com.tr" class="btn btn-default"><span class="glyphicon glyphicon-envelope"></span> Destek Al </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</main>


